{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "echo \"No unit tests available. Run npm run test:e2e for E2E tests.\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui --project=chromium",
    "test:e2e:smoke": "playwright test --grep @smoke --project=chromium --workers=6 --max-failures=1",
    "test:e2e:lf": "playwright test --last-failed --project=chromium --workers=1 --max-failures=1",
    "test:e2e:full": "playwright test --project=chromium --workers=6"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}